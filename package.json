{
  "name": "safesip-web-app",
  "version": "2.0.0",
  "description": "SafeSip - Production-Ready Waterborne Disease Management System with AI/ML",
  "main": "app.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run ml-models\"",
    "dev:safe": "node start-dev.js",
    "dev:full": "concurrently -n \"SERVER,CLIENT,ML\" -c \"green,blue,yellow\" \"npm run server\" \"npm run client\" \"npm run ml-models\"",
    "server": "cross-env NODE_ENV=development PORT=5000 nodemon app.js",
    "client": "wait-on http://localhost:5000 && cd frontend && npm start",
    "ml-models": "wait-on http://localhost:5000 && node utils/mlModelRunner.js",
    "frontend": "cd frontend && npm start",
    "frontend:install": "cd frontend && npm install",
    "frontend:build": "cd frontend && npm run build",
    "build": "npm run frontend:install && npm run frontend:build",
    "build:docker": "docker build -t safesip:latest .",
    "build:dev": "docker build -f Dockerfile.dev -t safesip:dev .",
    "setup": "npm install && npm run frontend:install",
    "vercel-build": "npm run build",
    "heroku-postbuild": "npm run build",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "launch": "node launcher.js",
    "start:all": "node launcher.js",
    "dev:complete": "node launcher.js",
    "docker:up": "docker-compose up",
    "docker:up:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:down": "docker-compose down",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset --force",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^5.0.0",
    "@prisma/client": "^5.22.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "connect-flash": "^0.1.1",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "express-slow-down": "^2.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "redis": "^4.7.1",
    "sharp": "^0.33.5",
    "simple-statistics": "^7.8.8",
    "socket.io": "^4.8.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.22.0",
    "supertest": "^6.3.4",
    "wait-on": "^7.2.0"
  },
  "keywords": [
    "water",
    "health",
    "disease",
    "management",
    "prediction",
    "ml",
    "realtime",
    "healthcare",
    "waterborne",
    "prevention"
  ],
  "author": "SafeSip Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/safesip/safesip-web-app.git"
  },
  "bugs": {
    "url": "https://github.com/safesip/safesip-web-app/issues"
  },
  "homepage": "https://safesip.com"
}
